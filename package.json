{
  "name": "Spree-e2e",
  "description": "End to End tests",
  "scripts": {
    "chrome": "node ensureSeleniumJarExist.js && ./node_modules/.bin/nightwatch ./tests/SpreeTest.js --reporter html-reporter.js --config nightwatch.conf.js --env chrome",
    "chromeHeadless": "node ensureSeleniumJarExist.js && ./node_modules/.bin/nightwatch ./tests/SpreeTest.js --reporter html-reporter.js --config nightwatch.conf.js --env chromeHeadless"
  },
  "dependencies": {
    "nightwatch": "1.4.0",
    "selenium-download": "2.0.15",
    "geckodriver": "^1.19.1",
    "nightwatch-html-reporter": "^2.0.6"
  }
}